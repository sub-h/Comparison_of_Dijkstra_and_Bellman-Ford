if __name__ == '__main__':
    n = int(input())
    arr = map(int, input().split())
def second_largest(arr):
    dup_items = set()
    uniq_items = []
    for x in arr:
        if x not in dup_items:
            uniq_items.append(x)
            dup_items.add(x)
    uniq_items.sort()    
    print(uniq_items[-2])
second_largest(arr)
